<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="212.0" prefWidth="252.0" spacing="10" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab13practice.LoginController">
    <Label text="Username:">
      <font>
         <Font name="System Bold" size="12.0" />
      </font></Label>
    <TextField fx:id="usernameField" maxWidth="-Infinity" prefHeight="26.0" prefWidth="199.0" />
    <Label text="Password:">
      <font>
         <Font name="System Bold" size="12.0" />
      </font></Label>
    <PasswordField fx:id="passwordField" maxWidth="-Infinity" prefHeight="26.0" prefWidth="199.0" />
    <Button onAction="#handleLogin" prefHeight="23.0" prefWidth="108.0" text="Login" textAlignment="JUSTIFY" />
</VBox>
